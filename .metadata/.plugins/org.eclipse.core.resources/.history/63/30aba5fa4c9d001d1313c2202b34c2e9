package gas_station;

public class Fuel {
	private int sort;
	private int remain;
	
	public int getSort() {
		return sort;
	}
	public void setSort(int sort) {
		this.sort = sort;
	}
	public int getRemain() {
		return remain;
	}
	public void setRemain(int remain) {
		this.remain = remain;
	}
	Fuel(int sort){
		this.sort = sort;
	}
	Fuel(){
		
	}

	
}
