package market;

public class Market {
	private int price;
	private Buyer buyer = new Buyer();
	private Seller seller = new Seller();
	public void proceed(Buyer buyer,Seller seller, int amount) {
		buyer.buy(seller,amount);
		seller.sell(buyer,amount);
		buyer.show();
		seller.show();
	}
	public static void main(String[] args) {
		proceed()
	}
}
