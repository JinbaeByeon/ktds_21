package market;

public class Market {
	private int price;
	private Buyer buyer = new Buyer();
	private Seller seller = new Seller();

	public void proceed() {
		int amount;
		buyer.buy(seller, amount);
		seller.sell(buyer, amount);
	}

	public void show() {
		buyer.show();
		seller.show();
	}

	public static void main(String[] args) {
		proceed();
		show();
	}
}
