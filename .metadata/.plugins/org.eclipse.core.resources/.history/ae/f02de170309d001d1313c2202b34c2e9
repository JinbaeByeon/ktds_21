package calculator;

/**
 * 사칙연산 클래스
 * 
 * @author User
 *
 */
public class Calculator {
	
	//더하기
	public int add(int a,int b) {
		return a+b;
	}
	//빼기
	public int subtract(int a,int b) {
		return a-b;
	}
	//곱하기
	public int multiply(int a,int b) {
		return a*b;
	}
	//나누기
	public double division(int a,int b) {
		return (double)a/b;
	}
	//나머지 구하기
	public int remains(int a,int b) {
		return a%b;
	}
}
